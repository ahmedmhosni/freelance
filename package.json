{
  "name": "freelancer-management-app",
  "version": "1.0.0",
  "description": "Freelancer management platform with admin oversight",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install"
  },
  "keywords": [
    "freelancer",
    "management",
    "admin"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "@azure/communication-email": "^1.1.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "pg": "^8.16.3"
  }
}
